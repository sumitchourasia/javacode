public class ToBInary {

	public static void toBinary(int n)
	{
		int[] arr=new int[15];
		int bit=0;
		while(n>0)
		{
			bit=n%2;
			n=n/2;
			
			
		}
		
		
	}
	
	
	public static void main(String[] args) {
		
		System.out.println("enter the number : ");
		int n=Utility.readInt();
		
		toBinary(n);
		
		
		
	}

}
