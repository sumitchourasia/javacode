package com.bridgelab.functional;

import com.bridgelab.utility.Utility;

public class TwoDArray {
	
	public static void main(String[] args) {
	
		System.out.println("enter \n 1 for integer array \n 2 for double array \n 3 for boolean array");
		int k=Utility.readInt();
		
		System.out.println("enter the length and breadth of array : ");
		int i=Utility.readInt();
		int j=Utility.readInt();
		
		switch (k)
		{
			case 1:
			{
				int [][] arr=new int[i][j];
				Utility.enter2DArrayInt(arr, i, j);
				break;
			}
			case 2:
			{
				double [][] arr=new double[i][j];
				Utility.enter2DArrayDouble(arr, i, j);
				break;
			}
			case 3:
			{
				boolean[][] arr=new boolean[i][j];
				Utility.enter2DArrayBoolean(arr, i, j);
				break;
			}
			default :
			{
				System.out.println("invalid selection");
			}
		}
		
		System.out.println("enter the data : ");
		String data=Utility.readString();
			
		}
		
		
		
}














