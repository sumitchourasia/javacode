public static void parseString(String data)
{
	try {
		int integerdata=Integer.parseInt(data);
		int[][] arr=new int[i][j];
		System.out.println("interger : "+integerdata);
	}
	catch(NumberFormatException e) {
		try {
			double doubledata=Double.parseDouble(data);
			System.out.println("double : "+doubledata);
		}
		catch(NumberFormatException f ) {
			try {
				Boolean booleandata=Boolean.parseBoolean(data);
				System.out.println("boolean : "+booleandata);
			}
			catch(NumberFormatException n)
			{
				System.out.println("string "+data);
			}
		}
	}
	
}
	
	
	
